  <body class="register-body">
    <article class="container">
      <form method="POST" action="/register">
        <input
          type="text"
          name="username"
          placeholder="Username"
          id="username"
        />
        <input
          type="password"
          name="password"
          placeholder="Password"
          id="password"
        />
        <button type="submit" class="btn-register">Register my Account</button>
      </form>
    </article>
  </body>
